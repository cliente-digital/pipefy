{
    "name": "clientedigital/pipefy",
    "description": "A Pipefy API wrapper that uplift productivity of newcomers to graphQL tech.",
    "type": "library",
    "license": "Apache-2.0",
    "keywords":["graphql", "pipefy"],
    "autoload": {
        "psr-4": {
            "Clientedigital\\Pipefy\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ivo Nascimento",
            "email": "iannsp@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^7.8",
        "guzzlehttp/psr7": "^2.6"
    },
    "bin": [
        "bin/config.php"
    ],

    "scripts": {
        "clientedigital:pipefy:config":"@php config.php",
        "clientedigital:pipefy:schema":"@php schema.php",
        "clientedigital:pipefy:report":"@php report.php"
    },
    "scripts-descriptions":{
        "clientedigital:pipefy:config":"Parametriza sua conta Pipefy e opções de operação do pacote.",
        "clientedigital:pipefy:schema":"Realize Operações sobre o schema de dados.",
        "clientedigital:pipefy:report":"Gera Relatórios."
    },
    "config": {
        "bin-dir": "bin/",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "educoder/pest": "^1.0"
    }
}
