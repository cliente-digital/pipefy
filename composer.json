{
    "name": "clientedigital/pipefy",
    "description": "A Pipefy API wrapper that uplift productivity of newcomers to graphQL tech.",
    "type": "library",
    "license": "Apache-2.0",
    "version":"0.0.1-alpha",
    "keywords":["graphql", "pipefy"],
    "autoload": {
        "psr-4": {
            "Clientedigital\\Pipefy\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ivo Nascimento",
            "email": "iannsp@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^7.8",
        "guzzlehttp/psr7": "^2.6"
    },
    "scripts": {
        "pipefy:config":"@php config.php",
        "pipefy:schema":"@php schema.php",
        "pipefy:report":"@php report.php"
    },
    "scripts-descriptions":{
        "pipefy:config":"Parametriza sua conta Pipefy e opções de operação do pacote.",
        "pipefy:schema":"Realize Operações sobre o schema de dados.",
        "pipefy:report":"Gera Relatórios."
    },
    "config": {
        "bin-dir": "bin/"
    }
}
